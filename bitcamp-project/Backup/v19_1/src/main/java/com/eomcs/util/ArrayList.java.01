package com.eomcs.util;

public class ArrayList {
  Object[] elementData;
  // Object[] elementData = new Object[100];
  // 이렇게 사용하면 무조건 사이즈가 100로 고정되기 때문에
  // 크기를 조절하고 싶으면 생성자에 넣어서 조절하게 만들기
  int size;

  public ArrayList() {
